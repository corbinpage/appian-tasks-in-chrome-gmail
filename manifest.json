{
  "name": "Appian Tasks in Gmail",
  "description": "Display Appian Tasks in Gmail",
  "version": "0.1",
  "browser_action": {
    "default_icon": "chrome-extension/spy-icon-19.png",
    "default_title": "Appian Tasks in Gmail",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://mail.google.com/",
    "https://api.myjson.com/"
  ],
  "optional_permissions": [
    "https://*/",
    "http://*/"
  ],
  "manifest_version": 2
}